            <ui:TextField picking-mode="Ignore" label="Conference" text="Unity" name="ConferenceName" value="Unity" style="position: relative; flex-direction: row; background-color: rgba(0, 0, 0, 0); top: auto; max-width: none; color: rgb(255, 255, 255); -unity-text-align: upper-left; -unity-text-outline-color: rgb(0, 0, 0);" />
            <ui:DropdownField label="Audio Input" index="-1" choices="System.Collections.Generic.List`1[System.String]" name="AudioInput" />
