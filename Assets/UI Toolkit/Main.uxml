<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI%20Toolkit/UnityThemes/style.uss?fileID=7433441132597879392&amp;guid=a501c1e8bd725461b8a7b790da1373c7&amp;type=3#style" />
    <ui:VisualElement style="justify-content: center; align-items: center; flex-grow: 1; display: flex; visibility: visible;">
        <ui:VisualElement style="flex-direction: column; background-color: rgba(0, 0, 0, 0.88); border-top-left-radius: 10px; border-bottom-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px; padding-left: 10px; padding-right: 10px; padding-top: 10px; padding-bottom: 10px; align-items: stretch; width: 50%;">
